<!DOCTYPE html>
<html lang="en">
<head>
  <title>lit-html fetch</title>
  
</head>

  <body>
    <my-page></my-page>
<script type="module">
import {LitElement,html,until} from '../lit-element-2.3.1.js';
const content = fetch('./content.txt').then(r => r.text());

html`${until(content, html`<span>Loading...</span>`)}`
</script>

  </body>

</html>
